<templateSet group="MyGroup">
  <template name="*" value="*&#10; * Description:&#10; * @param $params$&#10; * @return $return$&#10; * @auther Jack Chan&#10; * @date $date$ $time$&#10; */" description="Method Description" toReformat="false" toShortenFQNames="true">
    <variable name="params" expression="methodParameters()" defaultValue="groovyScript(&quot;def result=''; def params=\&quot;${_1}\&quot;.replaceAll('[\\\\[|\\\\]|\\\\s]', '').split(',').toList(); for(i = 0; i &lt; params.size(); i++) {result+=' * @param ' + params[i] + ((i &lt; params.size() - 1) ? '\\n':'')}; return result&quot;, methodParameters()) 1 " alwaysStopAt="false" />
    <variable name="return" expression="methodReturnType()" defaultValue="methodReturnType()" alwaysStopAt="false" />
    <variable name="date" expression="date()" defaultValue="date()" alwaysStopAt="false" />
    <variable name="time" expression="time()" defaultValue="time()" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>